{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Osmosis API",
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "favicon": "/favicon.png",
  "openapi": "/openapi.json",
  "api": {
    "baseUrl": "https://osmosis.gulp.dev",
    "playground": {
      "mode": "show"
    },
    "auth": {
      "method": "key",
      "name": "x-api-key"
    }
  },
  "colors": {
    "primary": "#707FDD",
    "light": "#8892E3",
    "dark": "#5C6AD4",
    "anchors": {
      "from": "#707FDD",
      "to": "#8892E3"
    }
  },
  "topbarLinks": [
    {
      "name": "Contact Us",
      "url": "mailto:founders@gulp.ai"
    }
  ],
  "tabs": [
    {
      "name": "Methods Playground",
      "url": "api-reference"
    },
    {
      "name": "Examples",
      "url": "examples"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": [
        "introduction",
        "quickstart/authentication"
      ]
    },
    {
      "group": "API Reference",
      "pages": [
        "api-reference/overview",
        "api-reference/enhance-task",
        "api-reference/store-knowledge",
        "api-reference/delete-by-intent"
      ]
    }
  ]
}
